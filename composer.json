{
    "name": "turkpin/admin-kit",
    "description": "Modern enterprise admin panel solution with intelligent Docker setup and smart environment management",
    "type": "library",
    "license": "MIT",
    "version": "1.0.6",
    "keywords": [
        "admin",
        "panel",
        "admin-panel", 
        "crud",
        "dashboard",
        "backend",
        "php",
        "symfony",
        "easyadmin",
        "docker",
        "enterprise",
        "modern",
        "intelligent",
        "zero-config"
    ],
    "authors": [
        {
            "name": "Oktay Aydogan",
            "email": "oktay@turkpin.com",
            "homepage": "https://github.com/oktayaydogan",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/oktayaydogan/admin-kit",
    "support": {
        "issues": "https://github.com/oktayaydogan/admin-kit/issues",
        "source": "https://github.com/oktayaydogan/admin-kit",
        "docs": "https://github.com/oktayaydogan/admin-kit/tree/main/docs"
    },
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-intl": "*",
        "doctrine/orm": "^2.15",
        "smarty/smarty": "^4.3",
        "symfony/console": "^6.0|^7.0",
        "symfony/yaml": "^6.0|^7.0",
        "predis/predis": "^2.0",
        "ratchet/pawl": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10",
        "squizlabs/php_codesniffer": "^3.7"
    },
    "suggest": {
        "ext-redis": "For Redis caching support",
        "ext-gd": "For image manipulation support",
        "ext-zip": "For export/import functionality"
    },
    "autoload": {
        "psr-4": {
            "AdminKit\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "AdminKit\\Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/adminkit"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "AdminKit\\Providers\\AdminKitServiceProvider"
            ]
        }
    },
    "scripts": {
        "install-adminkit": [
            "@php bin/adminkit install"
        ],
        "test": "phpunit",
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "analyse": "phpstan analyse"
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true,
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

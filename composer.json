{
    "name": "turkpin/admin-kit",
    "description": "Modern admin panel generator for Slim Framework with Smarty templates and Doctrine ORM",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Turkpin",
            "email": "info@turkpin.com"
        }
    ],
    "keywords": [
        "admin",
        "panel",
        "slim",
        "smarty",
        "doctrine",
        "crud",
        "generator",
        "admin-panel",
        "backend"
    ],
    "require": {
        "php": ">=8.1",
        "slim/slim": "^4.0",
        "slim/psr7": "^1.6",
        "smarty/smarty": "^4.3",
        "doctrine/orm": "^2.15",
        "doctrine/dbal": "^3.6",
        "doctrine/migrations": "^3.6",
        "symfony/console": "^6.0|^7.0",
        "symfony/security-core": "^6.0|^7.0",
        "symfony/password-hasher": "^6.0|^7.0",
        "psr/container": "^2.0",
        "psr/log": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Turkpin\\AdminKit\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Turkpin\\AdminKit\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
